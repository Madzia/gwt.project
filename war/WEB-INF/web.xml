<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">
     
         
    <!-- Servlets -->
  
   <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>project.server.LoginServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/gwt_project/login</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>entityServlet</servlet-name>
    <servlet-class>project.server.EntityServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>entityServlet</servlet-name>
    <url-pattern>/gwt_project/entity</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Gwt_project.html</welcome-file>
  </welcome-file-list>

</web-app>
